{
    "name": "gifshot",
    "version": "0.4.5",
    "description": "JavaScript library that can create animated gifs from video streams (e.g. webcam), existing videos (e.g. mp4), or existing images",
    "main": "./dist/gifshot",
    "repository": {
        "type": "git",
        "url": "https://github.com/yahoo/gifshot.git"
    },
    "keywords": [
        "gif",
        "animated gif",
        "WebRTC",
        "getUserMedia"
    ],
    "contributors": [
        {
            "name": "Chase West"
        },
        {
            "name": "Greg Franko"
        }
    ],
    "scripts": {
        "preview": "node demo/server/server.js",
        "build": "gulp",
        "watch": "gulp watch",
        "test": "gulp test",
        "pretty": "prettier --write ./**/*.{json,js,html,css,ts}"
    },
    "precommit": [
        "build",
        "pretty"
    ],
    "devDependencies": {
        "@babel/core": "^7.17.2",
        "@babel/plugin-external-helpers": "^7.16.7",
        "@babel/preset-env": "^7.16.11",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "animated_gif": "^1.0.2",
        "chai": "^1.9.1",
        "express": "^4.2.0",
        "gulp": "^4.0.2",
        "gulp-iife": "^0.3.0",
        "gulp-insert": "^0.3.0",
        "gulp-mocha": "^1.1.0",
        "gulp-rename": "^1.2.0",
        "gulp-uglify": "^3.0.2",
        "jshint": "latest",
        "omggif": "^1.0.10",
        "precommit-hook": "^1.0.7",
        "prettier": "^2.5.1",
        "rollup": "^2.67.1"
    },
    "engine": {
        "node": ">= 4.0.0"
    },
    "license": "MIT",
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 120
    }
}
